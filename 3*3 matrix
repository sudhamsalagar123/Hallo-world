#include<stdio.h>
int main()
{
	int a[3][3];
	int i,j,s=0;
	printf("Enter the elements for the matrix \n");
	for (i=0;i<3;i++);
	{
		for(j=0;j<3;j++)
		{
			printf("elements [%d][%d]",i,j);
			scanf("%d",&a[i][j]);
			
	    }
	}
	for(i=0;i<=2;i++)
	{
		for(j=0;j<=2;j++)
		{
			printf("%d",a[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		if(i==j)
		{
			s=s+a[i][j];
		}
		printf("\n sum of diagonal one is =%d",s);
	}
}
